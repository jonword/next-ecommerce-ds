generator client {
  provider = "prisma-client-js"
  output = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Products {
  id          Int    @id @default(autoincrement())
  name        String
  price       Int
  image       String
  category    String
  description String
}
