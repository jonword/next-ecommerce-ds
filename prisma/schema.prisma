generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Product {
  id          String @id @unique
  name        String
  price       Int
  image       String
  category    String
  description String
}

model Products {
  id          Int    @id @default(autoincrement())
  name        String
  price       Int
  image       String
  category    String
  description String
}
